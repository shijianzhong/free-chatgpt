// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"title": {
			"title": "标题",
			"bsonType": "string",
			"trim": "both"
		},
		"detail": {
			"title": "详情",
			"bsonType": "string",
			"trim": "both"
		},
		"create_time": {
			"bsonType": "string",
			"title": "创建时间"
		}
	}
}
